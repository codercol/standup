<!DOCTYPE html>
<html>
  <head>
    <title>StandupApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://kit.fontawesome.com/youruniqueID.js"></script>
  </head>

  <body class="sidebar-min">
    <div class="wrapper">
      <%= render partial: "layouts/header" %>
      <%= render partial: "layouts/sidecar" %>
      <div class="content-wrapper">
       <%= yield %>
      </div>
      <% render partial: "layouts/footer" %> 
     </div> 
  </body>
</html>
